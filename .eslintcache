k-theme.css\";\nimport Slider from \"react-slick\";\nimport \"./css/MovieCard.css\";\n\nfunction MovieCard({uri, title}) {\n   const [movieData, setMovieData] = useState([])\n    var settings = {\n        infinite: true,\n        speed: 500,\n        slidesToShow: 4,\n        slidesToScroll: 1,\n        arrows :true,\n        pauseOnFocus : true,\n        autoplay: true\n      };\n      useEffect(() => {\n        fetch(uri)\n        .then((response) => response.json())\n        .then((data) => {\n            console.log(data.results)\n            setMovieData(data.results)\n        })\n      }, [])\n    return (\n        <div className=\"moviecard\">\n            <h2>{title}</h2>\n            <Slider {...settings}>\n                {movieData.map((movie) => (\n                    <div className=\"moviecard__sliderImageDivision\">\n                        <img src={`https://image.tmdb.org/t/p/w400${movie.backdrop_path}`} alt={`${movie.title}`} />\n                    </div>\n                ))}\n            </Slider>\n        </div>\n    )\n}\n\nexport default MovieCard\n\n\n\n","/Users/neerajgupta/Desktop/primevideoclone/src/endpoints.js",[],"/Users/neerajgupta/Desktop/primevideoclone/src/components/Footer.jsx",["61","62","63"],"import React from 'react';\nimport \"./css/Footer.css\";\nimport Logo from \"../assets/logo.png\"\n\nfunction Footer() {\n    return (\n        <div className=\"footer\">\n            <img src={Logo} alt=\"\"/>\n            <ul className=\"footer__links\">\n                <li><a href=\"#\">Terms and Privacy Notice</a></li>\n                <li><a href=\"#\">Send Us Feedback</a></li>\n                <li><a href=\"#\">Help</a></li>\n                <li>© 1996-2021, AmazonFake.com, Inc. or its affiliates</li>\n            </ul>\n        </div>\n    );\n}\n\nexport default Footer\n","/Users/neerajgupta/Desktop/primevideoclone/src/components/LoginPage.jsx",["64","65"],"import React from 'react';\nimport Logo from \"../assets/logo.png\";\nimport